<header class=header><section class=warp-header><div class=logo> <% if (hadLoc) { %> <a href="/<%- loc %>/home"><img src="<%- images.logoImg %>"> </a> <% }else{ %> <a href=/home><img src="<%- images.logoImg %>"> </a> <% } %> </div><div class=title> <% if (hadLoc) { %> <a href="/<%- loc %>/home"> <%- language.title %> </a> <% }else{ %> <a href=/home> <%- language.title %> </a> <% } %> </div><div class=nav><ul> <% language.nav.forEach(value => { %> <li> <% if (hadLoc) { %> <a href="/<%- loc %>/<%- value.path %>"><div class="nav-item text"><%- value.name %></div></a> <% }else{ %> <a href="/<%- value.path %>"><div class="nav-item text"><%- value.name %></div></a> <% } %> </li> <% }) %> </ul></div><div class=language><div class="toggle text"> <%- language.language %> </div></div></section></header><div class=change-language><div class=language-list><ul> <% global.locCode.forEach(value => { %> <li class=language-item><a href="/<%- value.code %>/<%- path %>"><div class=language> <%- value.name %> </div></a></li> <% }) %> </ul></div></div>